<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    >

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        tools:ignore="MissingConstraints">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="100dp"
            android:layout_marginLeft="60dp"
            android:src="@drawable/robot1"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="100dp"
            android:layout_marginLeft="300dp"
            android:src="@drawable/robot2"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="100dp"
            android:layout_marginLeft="530dp"
            android:src="@drawable/robot3"/>

        <TextView
            android:id="@+id/text_email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:layout_marginTop="420dp"
            android:layout_marginLeft="320dp"
            android:text="이메일 : "
            />

        <ImageView
            android:id="@+id/line"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/line"
            android:layout_marginTop="440dp"
            android:layout_marginLeft="240dp"/>

        <ImageButton
            android:id="@+id/btn_logout"
            android:layout_width="146dp"
            android:layout_height="44dp"
            android:layout_marginTop="500dp"
            android:layout_marginLeft="320dp"
            android:background="@drawable/logout"/>

        <ImageView
            android:id="@+id/lee_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="600dp"
            android:layout_marginLeft="200dp"
            android:src="@drawable/lee"
            android:background="@android:color/transparent"/>

        <ImageView
            android:id="@+id/sun_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="600dp"
            android:layout_marginLeft="400dp"
            android:src="@drawable/sun"
            android:background="@android:color/transparent"/>

        <ImageButton
            android:id="@+id/kim_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="900dp"
            android:layout_marginLeft="100dp"
            android:src="@drawable/kim"
            android:background="@android:color/transparent"/>

        <ImageButton
            android:id="@+id/park_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="900dp"
            android:layout_marginLeft="300dp"
            android:src="@drawable/park"
            android:background="@android:color/transparent"/>

        <ImageButton
            android:id="@+id/choi_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="900dp"
            android:layout_marginLeft="500dp"
            android:src="@drawable/choi"
            android:background="@android:color/transparent"/>

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>